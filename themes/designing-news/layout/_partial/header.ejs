<div class="site-header">
  <h1 class="site-title"><a href="<%- config.root %>"><%= config.title %></a></h1>
  <h2 class="site-subtitle"><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% }
      %></a></h2>
</div>
<div class="main-nav">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
</div>