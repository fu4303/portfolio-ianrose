<h1 class="heading heading--page">Articles</h1>
<div id="posts">
<%# <input type="text" class="fuzzy-search" placeholder="Search"/> %>
<section class="section row list">
    <% var parentSlug = 'articles' %>
    <% for(var slug in public.articles._data){ %>
    <% var entry = public.articles._data[slug] %>
    <% var date = entry.date %>
    <% var modified = entry.modified %>
    <% if(entry.published) { %>
        <% include ../_includes/entry %>
    <% } %>
    <% } %>
</section>
</div>
